{
    "name": "ts-async-pipeline",
    "version": "1.0.4",
    "description": "A small library for running async functions in sequence with the possibility of limiting their execution time and specifying a fallback value.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davi4046/ts-async-pipeline.git"
    },
    "keywords": [
        "async",
        "pipeline",
        "sequence",
        "timeout",
        "phases",
        "piping"
    ],
    "author": "David Rud Pedersen",
    "license": "MIT",
    "devDependencies": {
        "@swc/core": "^1.6.7",
        "@types/jest": "^29.5.12",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.5",
        "tsup": "^8.1.0",
        "typescript": "^5.5.3"
    }
}
